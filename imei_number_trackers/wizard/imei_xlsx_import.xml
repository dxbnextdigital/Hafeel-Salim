<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

		<record id="imei_xls_import_form_view" model="ir.ui.view">
			<field name="name">imei.xls.import.wizard.view</field>
			<field name="model">imei.xls.import</field>
			<field name="arch" type="xml">
			<form string="Import IMEI">
				<group>

					<group>
						<field name="file" placeholder="Upload Your File"/>
					</group>

				</group>
				<footer>
					<button name="export_export_product"  type="object" string="Export Xlsx" icon="fa-download" class="btn btn-primary active"/>

					<button name="import_imei_numbers" string="Import"  type="object" icon="fa-download" class="btn btn-primary active"/>
					<button string="Cancel" special="cancel" icon="fa-times" class="btn btn-danger"  />
				</footer>
			</form>
			</field>
		</record>
    </data>
</odoo>