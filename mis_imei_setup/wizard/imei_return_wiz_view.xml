<odoo>
     <data>
          <record id="imei_return_wiz_view" model="ir.ui.view">
               <field name="name">imei return wiz view</field>
               <field name="model">imei.return.wiz</field>
               <field name="arch" type="xml">
                    <form string="Return IMEI Number">
                         <div class="oe_title">
                              <field name="product_id" readonly="1"/>
                              <field name="partner_id" readonly="1"/>
                         </div>
                         <group>
                              <group>
                                   <field name="ret_product_quantity" readonly="1"/>
                                   <field name="total_ret_imei"/>
                              </group>
                         </group>
                         <group>
                         <notebook>
                              <page string="IMEI To Return ">
                                   <field name="ret_imei_line_ids">
                                        <tree editable="bottom">
                                             <field name="imei" required='1'/>
                                             <field name="is_return"/>
                                        </tree>
                                   </field>
                              </page>
                         </notebook>
                    </group>
                         <footer>
                              <button name="return_imei" string="Save" type="object" 
                                      class="btn-primary"/>
                              <button string="Cancel" class="btn-default" special="cancel" />
                         </footer>
                    </form>
               </field>
          </record>
     </data>
</odoo>